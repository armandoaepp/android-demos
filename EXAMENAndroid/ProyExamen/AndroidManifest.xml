            android:name="com.example.proyexamen.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.proyexamen.ListNoticiasActivity"
            android:label="@string/app_name1" >
        </activity>   
        <activity
            android:name="com.example.proyexamen.DetNoticiaActivity"
            android:label="@string/app_name2" >
        </activity>  
    </application>

</manifest>
